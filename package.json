{
  "name": "dropstab-api",
  "version": "1.0.0",
  "description": "TypeScript library for interacting with Dropstab API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/whooooop/dropstab.git"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "prepublish": "npm run clean && npm run build && npm run test",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "dropstab",
    "api",
    "typescript",
    "blockchain"
  ],
  "author": "Whooooop",
  "license": "MIT"
}
